{
  "buildCommand": "npm run build",
  "outputDirectory": ".vercel/output",
  "framework": "nuxtjs",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/_nuxt/builds/meta/:path*",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/_nuxt/builds/meta/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
